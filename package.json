{
  "name": "vscode-typescript-boilerplate",
  "version": "1.2.1",
  "description": "A project skeleton for TypeScript development in Visual Studio Code.",
  "main": "build/app/app.js",
  "scripts": {
    "build": "tsc -p src || true",
    "postbuild": "cpx 'src/{app,test}/**/*.json' build",
    "rebuild": "rimraf build && tsc -p src || true",
    "postrebuild": "cpx 'src/{app,test}/**/*.json' build",
    "test": "mocha build/test --require source-map-support/register || true",
    "clean": "rimraf build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codesleuth/vscode-typescript-boilerplate.git"
  },
  "keywords": [
    "vscode",
    "typescript",
    "example",
    "boilerplate",
    "project",
    "skeleton",
    "debug",
    "intellisense",
    "compile",
    "transpile"
  ],
  "author": "David Wood <david.p.wood@gmail.com> (http://www.codesleuth.co.uk/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/codesleuth/vscode-typescript-boilerplate/issues"
  },
  "homepage": "https://github.com/codesleuth/vscode-typescript-boilerplate#readme",
  "private": true,
  "devDependencies": {
    "@types/glob": "^5.0.30",
    "@types/iconv-lite": "0.0.1",
    "@types/log4js": "0.0.32",
    "@types/mocha": "^2.2.41",
    "@types/node": "^7.0.27",
    "@types/rimraf": "0.0.28",
    "@types/sinon": "^2.3.0",
    "@types/tmp": "0.0.33",
    "mocha": "^3.4.2",
    "rimraf": "^2.6.1",
    "sinon": "^2.3.2",
    "source-map-support": "^0.4.15",
    "typescript": "^2.3.4"
  },
  "dependencies": {
    "glob": "^7.1.2",
    "iconv-lite": "^0.4.17",
    "log4js": "^1.1.1",
    "reflect-metadata": "^0.1.10",
    "rimraf": "^2.6.1",
    "sqlite": "^2.8.0",
    "tmp": "0.0.31",
    "tslib": "^1.7.1",
    "typedi": "^0.5.2",
    "typeorm": "0.1.0-alpha.3",
    "typeorm-typedi-extensions": "0.0.12"
  }
}
